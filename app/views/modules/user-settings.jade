// user-settings.jade
- if(!ajax) {
!= partial('partials/styles')
- }

header#main

	#toggle-side-bar.ff-icon-before
		span.toggle-title
			strong Collapse 
			| sidebar
		span.toggle-sub * You can also re-open from here
	
	#sectional-func.side-open	
		#x-side-bar(title='Toggle display of sidebar',onclick=)
			span.left-bars
				span
			span.right-bar
		- var klass = (tabs.length == 1 ? 'singular' : '');
		ul.sectional-tabs(class=klass)
			- for(var i = 0; i < tabs.length; ++i) {
				- var tab = tabs[i];
				li(id=tab.id)
					a(href="javascript:$.pv3.panel.show('" + tab.panel + "', {'tabid':'" + tab.id + "'});")
						strong= tab.h1
						= tab.h2
			- }
			
		form#quick-search.well
			input#quick-search-keyword(value='', placeholder='Quick domain search _')
			#sale-type.btn-group
				a.btn.dropdown-toggle(data-toggle='dropdown', href='javascript:void(0);')
					| Sale type
					span.caret
				ul.dropdown-menu
					li
						a(href='') Auctions only
					li
						a(href='') Pre-auctions only
					li
						a(href='') Expiring only
					li
						a(href='') Dropping only
					li.divider
	
					li
						a(href='') New Registrations only
					li
						a(href='') Websites only
			button.btn.ff-icon-before(type='submit') Submit

	
!= partial('partials/panel')

#main-container.page-exchange.search-results

		h5#first-heading.ff-icon-before
