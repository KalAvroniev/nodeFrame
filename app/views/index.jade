						$('#maincontent').html(data);
						$('.spine_nav').removeClass("selected");
						$('.spine_nav#' + module).addClass("selected");
					}
				});
			}
			$(document).ready(function () {
				$('#main-container').on('ajaxLoaded', function() {
					$('.ajax-spinner').hide();
				});
				$('#main-container').on('ajaxUnloading', function() {
					$('.ajax-spinner').show();
				});
